    <TIER ANNOTATOR="strnglm" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@np1970f"
        PARTICIPANT="@np1970f" TIER_ID="transl@np1970f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a88" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>there was a village</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a89" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>people lived and worked on a collective farm</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a90" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>went to school this road We</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a92" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>and by now some of them have died, some have gone to the children, there are only two yards left</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a93" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>they don't have any doorbells </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a94" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>somehow I'll get through to them</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>the oldest hut here</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="strnglm" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@ksm1932f"
        PARTICIPANT="@ksm1932f" TIER_ID="transl@ksm1932f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a96" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>ouch , I could've put something on </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a97" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>#indistinct#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a110" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>well I have three children</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a111" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>all my life I was on the farm, and in the war</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a112" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>hm, I'm a war veteran, a labour veteran </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a113" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>well the children left too - all three of them</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a115" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>I live alone</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a116" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>they come sometimes</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a117" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>help with everything </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a118" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>there were thirty two yards </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a119" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>when we were children, there were thirty two yards; the well was a kilometer away #indistinct# we went there</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a121" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>well there were jobs</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a123" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>and when the farms were united, everyone began to leave</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a124" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE>as they could not have a job</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a127" ANNOTATION_REF="a64">
                <ANNOTATION_VALUE>snow</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a155" ANNOTATION_REF="a153">
                <ANNOTATION_VALUE>#laughs#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a138" ANNOTATION_REF="a61">
                <ANNOTATION_VALUE>there are some people walking around here who bring irons, for example, and say like 'take them'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a139" ANNOTATION_REF="a74">
                <ANNOTATION_VALUE>#laughs#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a142" ANNOTATION_REF="a77">
                <ANNOTATION_VALUE>by the holidays</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a144" ANNOTATION_REF="a79">
                <ANNOTATION_VALUE>yeah, we close up </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a150" ANNOTATION_REF="a85">
                <ANNOTATION_VALUE>I do not want to live in flats. I would die there very soon sitting in a flat all day and night </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a151" ANNOTATION_REF="a86">
                <ANNOTATION_VALUE>because here I can go outside, walk in the open air, find something to do, or work in the garden, or check the chickens - there is always some business for me after all</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="strnglm" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@nng1950f"
        PARTICIPANT="@nng1950f" TIER_ID="transl@nng1950f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>we've known each other all our lives, worked together </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a99" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>then I got married //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a100" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>so we know each other, we're neighbors; how long has it already been... we're together for forty seven years</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a103" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>everything we had we had together</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a104" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>pulled out the weeds </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a105" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>went for bream</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a106" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>went  to the stacks </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a107" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>I do not have children</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a108" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>I lived with my husband for forty two years - he passed away five years ago </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a109" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>I was left alone</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a125" ANNOTATION_REF="a45">
                <ANNOTATION_VALUE>the young left, the old stayed, then all these old people died, so there's just the two of us - me and granny </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a128" ANNOTATION_REF="a63">
                <ANNOTATION_VALUE>we wake up in the morning</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a129" ANNOTATION_REF="a65">
                <ANNOTATION_VALUE>shovel the snow</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a130" ANNOTATION_REF="a66">
                <ANNOTATION_VALUE>I look to see how granny is, is she alive?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a154" ANNOTATION_REF="a152">
                <ANNOTATION_VALUE>#laughs#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a131" ANNOTATION_REF="a67">
                <ANNOTATION_VALUE>go with a shovel to clean the snow in front of her door, so that she can go out </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a132" ANNOTATION_REF="a68">
                <ANNOTATION_VALUE>and so we look after each other; I call her and ask 'hey are you alive?' she says 'alive'  'me too' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a134" ANNOTATION_REF="a70">
                <ANNOTATION_VALUE>Let's get married</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a135" ANNOTATION_REF="a71">
                <ANNOTATION_VALUE>we watch the programme of Yakubovich, 'Field of Wonders'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a136" ANNOTATION_REF="a72">
                <ANNOTATION_VALUE>and all those TV programmes</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a137" ANNOTATION_REF="a73">
                <ANNOTATION_VALUE>the next day we meet and tell each other about we watched</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a140" ANNOTATION_REF="a75">
                <ANNOTATION_VALUE>#laughs#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a141" ANNOTATION_REF="a76">
                <ANNOTATION_VALUE>and they usually come on  ... well, in honor of the ninth of May</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a143" ANNOTATION_REF="a78">
                <ANNOTATION_VALUE>we've got used to them by now, that's it</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a145" ANNOTATION_REF="a80">
                <ANNOTATION_VALUE>I am there, but my thoughts are here</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a146" ANNOTATION_REF="a81">
                <ANNOTATION_VALUE>I cried and cried and cried and then decided to leave</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a147" ANNOTATION_REF="a82">
                <ANNOTATION_VALUE>it was March, it was getting warmer</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a148" ANNOTATION_REF="a83">
                <ANNOTATION_VALUE>so I came back here</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a149" ANNOTATION_REF="a84">
                <ANNOTATION_VALUE>four years have passed since then</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
