        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a72"
                TIME_SLOT_REF1="ts85" TIME_SLOT_REF2="ts86">
                <ANNOTATION_VALUE>ну там / вот / все эти передачки / какие //</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a73"
                TIME_SLOT_REF1="ts87" TIME_SLOT_REF2="ts88">
                <ANNOTATION_VALUE>на другой день приходим / друг другу расскажем / что ...  что они делали //</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a75"
