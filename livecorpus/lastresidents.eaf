        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a67"
                TIME_SLOT_REF1="ts77" TIME_SLOT_REF2="ts78">
                <ANNOTATION_VALUE>с карцовкою у бабки / дверь тут вот прочишешь / чтоб бабка выходила //</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a68"
                TIME_SLOT_REF1="ts79" TIME_SLOT_REF2="ts80">
                <ANNOTATION_VALUE>ну вот / и друг другу / живы / по... позвоним по телефону / баб ты жива / жива / и я жива //</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a70"
