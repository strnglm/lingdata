        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a80"
                TIME_SLOT_REF1="ts103" TIME_SLOT_REF2="ts104">
                <ANNOTATION_VALUE>что / я там / а у меня думки тут //</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a81"
